        "#declare the sample size in variable n which represents number or orders\n",
        "n = 3\n",
        "\n",
        "#declare p which represents the probability of success\n",
        "p = 0.868\n",
        "\n",
        "#declare possbile number of orders in a numpy array\n",
        "orders = np.arange(0,4)\n",
        "orders\n"
